{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./src/*"],
      "@data/*": ["./data/*"]
    },
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "esModuleInterop": true,
    "declaration": false,
    "importHelpers": true,
    "jsx": "react",
    "module": "commonjs",
    "noEmitHelpers": false,
    "noImplicitAny": true,
    "noResolve": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "esnext",
  },
  "exclude": ["node_modules"]
}
